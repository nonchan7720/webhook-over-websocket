  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug for proxy",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "server",
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "debug for client",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "client",
        "--server-url",
        "http://localhost:8080"
      ],
      "console": "integratedTerminal"
    }
  ]
}
